---
<div class="jumbo">
  <div class="jumboName" ><span id="jumbo_name"></span></div>
  <div class="socialLinks">
    <div class="socialLink github">
      {# <a href="https://github.com/ccrowley96" target="_blank"><img src="/img/icons/github.svg" /></a>    #}
      <a href="https://github.com/ccrowley96" target="_blank">{% include "icons/github.svg"%}</a> 
    </div>  
    <div class="socialLink linkedin">
      <a href="https://www.linkedin.com/in/crowleycoryz/" target="_blank">{% include "icons/linkedin.svg"%}</a>    
    </div>
    <div class="socialLink twitter">
      <a href="https://twitter.com/crowlsyung" target="_blank">{% include "icons/twitter.svg"%}</a>
    </div>
    <div class="socialLink instagram">
      <a href="https://www.instagram.com/czcrowley/" target="_blank">{% include "icons/instagram.svg"%}</a>
    </div>
    <div class="socialLink soundcloud">
      <a href="https://soundcloud.com/ccrowley96" target="_blank">{% include "icons/soundcloud.svg"%}</a>
    </div>
    <div class="socialLink spotify">
      <a href="https://open.spotify.com/user/1221481626" target="_blank">{% include "icons/spotify.svg"%}</a>
    </div>
  </div>
  
</div>
{%if collections.posts.length >= 1%}
<div class="recentTitle">Recent Blogs</div>
<div class="recentItems">
  {% set postslist = collections.posts | head(-3) %}
  {% set postslistCounter = collections.posts | length %}
  {% include "postslist.njk" %}
  {# <p class="postCallout">More posts can be found in <a href="{{ '/posts/' | url }}">my blog</a>.</p> #}
</div>
{%endif%}
<div class="recentTitle">Recent Projects</div>
<div class="recentItems">
  {% set projectslist = collections.projects | head(-3) %}
  {% set projectslistCounter = collections.projects | length %}
  {% include "projectslist.njk" %}
  {# <p class="postCallout">More projects can be found in <a href="{{ '/projects/' | url }}">my projects</a>.</p> #}
</div>

<!-- Type JS -->
<script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.9"></script>
<script type="text/javascript">
    $(document).ready(function() {
    var options = {
        strings: ["Cory Z. Crowley"
                 ],
        typeSpeed:150,
        showCursor: false,
    }
    
    var typed = new Typed("#jumbo_name", options);
});
</script>
